((author "Diego A. Mundo")
 (synopsis "An opinionated wrapper around gsl.")
 (category math)
 (license "GPL-3.0")
 (dependencies srfi-1 srfi-13 bind miscmacros matchable)
 (test-dependencies test)
 (component-options
  (csc-options "-O3" "-C" "-O3"))
 (components

  ;; Mathematical Functions (real)
  (extension csl.math.double
             (source "math/csl.math.double.scm")
             (csc-options "-O3" "-C" "-O3" "-C" "`pkg-config --cflags gsl`")
             (link-options "-L" "`pkg-config --libs gsl`"))

  ;; Complex Numbers (+ math)
  (extension csl.math.complex
             (source "math/csl.math.complex.scm")
             (csc-options "-O3" "-C" "-O3" "-C" "`pkg-config --cflags gsl`")
             (link-options "-L" "`pkg-config --libs gsl`"))

